<!DOCTYPE html>
<html lang="en">
    <head>
        <%-include('./layout/head.ejs')%>
        </head>
<body>
    <header>
        <%-include('./layout/nav.ejs')%>

    </header>
    <main>
        <% senData.forEach(e=>{%> 
            <h1> <%=e.character%></h1>
            <p> <%=e.quote%></p>
            <p> <%=e.characterDirection%></p>
            <img src="<%=e.image%>">
           <form action="/favorite-quotes/<%=e.id%>?_methodOverride=PUT" method="POST">
                <input type="TEXT" name="quote" value="<%=e.quote%>">
                <input type="submit" value="UPDAT">
           </form>
           <form action="/favorite-quotes/<%=e.id%>?_methodOverride=DELETE" method="POST">
            <input type="submit" value="DELETE">

           </form>
        <%})%>
    </main>
</body>
</html>